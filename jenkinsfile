node {
    stage("GIT Clone"){
        
        git branch: 'main', credentialsId: 'GIT_CREDENTIALS', url: 'https://github.com/alagurajm1989/MyTestRepo1.git'
        
    }
        stage('Run Docker') {
           sshagent (['k8s-1']) {
              sh '''
               sudo docker run --privileged -d --name dind-test docker:dind
               sudo docker ps
               sudo cp -f incontainer.sh /root
               sudo docker cp /root/incontainer.sh dind-test:/opt
               sudo docker exec -it dind-test /bin/sh /opt/incontainer.sh
               '''
            }
        }
}
